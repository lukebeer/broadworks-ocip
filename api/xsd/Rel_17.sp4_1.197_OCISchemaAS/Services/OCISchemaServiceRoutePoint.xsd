  <xs:complexType name="UserRoutePointCallDispositionCodeGetAvailableListRequest">
    <xs:annotation>
      <xs:documentation>
        Get the list of available Route Point Call Disposition Codes to be used by a client.
        The response is either UserRoutePointCallDispositionCodeGetAvailableListResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="serviceUserId" type="UserId"/>
