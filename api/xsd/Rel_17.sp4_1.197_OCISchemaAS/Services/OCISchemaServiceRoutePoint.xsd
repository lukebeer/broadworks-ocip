  <xs:simpleType name="RoutePointCallFailureTimeout">
    <xs:annotation>
      <xs:documentation>
        The call failure timeout.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="60"/>
    </xs:restriction>
  </xs:simpleType>    
    
  <xs:simpleType name="RoutePointFailoverStatus">
    <xs:annotation>
      <xs:documentation>
        The Route Point failover status.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
